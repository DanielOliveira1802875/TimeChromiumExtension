{
    "manifest_version": 3,
    "name": "timeKeep",
    "description": "Insert the time worked in the appropriate locations.",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "https://time.keep.pt/*"
            ],
            "js": [
                "./index.bundle.js"
            ],
            "run_at": "document_end"
        }
    ]
}